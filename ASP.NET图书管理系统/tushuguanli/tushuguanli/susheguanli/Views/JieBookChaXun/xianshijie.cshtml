@model IQueryable<susheguanli.Models.JieShu>

<table border="1" style="width: 400px; border-collapse: collapse">
    <tr>
        <td style="text-align: center; font-size: 10px">书号</td>
        <td style="text-align: center; font-size: 10px">书名</td>
        <td style="text-align: center; font-size: 10px">借书人学号</td>
        <td style="text-align: center; font-size: 10px">借书人姓名</td>
        <td style="text-align: center; font-size: 10px">借书日期</td>
    </tr>
    @{
        foreach (var dange in Model)
        {
           
       
        <tr>
            <td style="font-size: 10px">@dange.shuhao</td>
            <td style="font-size: 10px">@dange.shuming</td>
            <td style="font-size: 10px">@dange.jieshurenxuehao</td>
            <td style="font-size: 10px">@dange.jieshurenname</td>
            <td style="font-size: 10px">@dange.jieshudate</td>
        </tr>
        }
    }
</table>

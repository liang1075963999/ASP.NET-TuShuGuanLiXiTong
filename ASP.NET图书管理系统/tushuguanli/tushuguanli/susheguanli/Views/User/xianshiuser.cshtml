@model IQueryable<susheguanli.Models.User>

<table border="1" style="width: 400px; border-collapse: collapse">
    <tr>
        <td style="text-align: center; font-size: 10px">用户</td>
        <td style="text-align: center; font-size: 10px">密码</td>
    </tr>
    @{
        foreach (var dange in Model)
        {
           
       
        <tr>
            <td style="text-align: center; font-size: 10px">@dange.userbianhao</td>
            <td style="text-align: center; font-size: 10px">@dange.password</td>
        </tr>
        }
    }
</table>
